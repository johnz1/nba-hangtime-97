robosnd_tbl
	;	duration, call#
	.word	sp_grunt|6,2972		;0 = grunt footstep	
	.word	sp_die|96,2980		;1 = die		
	.word	sp_rescue|39,2984	;2 = rescue
	.word	sp_shoot|17,2960	;3 = shoot		
	.word	sp_hit|23,2988		;4 = grunt dies
	.word	sp_human|212,2964	;5 = dead human

	.if	0	;table from WWF
	.word	sp_robo|71,1600		;290 = ROBOTRON shot			;0640h - 0b90 2960
	.word	sp_robo|212,1604	;291 = ROBOTRON human dies		;0644h - 0b94 2964
	.word	sp_robo|58,1608		;292 = ROBOTRON spawn enforcer		;0648h - 0b98 2968
	.word	sp_robo|6,1612		;293 = ROBOTRON robot footstep		;064ch - 0b9c 2972
	.word	sp_robo|96,1616		;294 = ROBOTRON effect (?)		;0650 - 0ba0 2976
	.word	sp_robo|164,1620	;295 = ROBOTRON player dies		;0654 - 0ba4 2980
	.word	sp_robo|39,1624		;296 = ROBOTRON rescue			;0658 - 0ba8 2984
	.word	sp_robo|23,1628		;297 = ROBOTRON grunt dies		;065c - 0bac 2988

	;reg use
	; a0 - (in) # of hulks

 SUBRP	make_hulks

	move	a0,a0
	jrz	henkdone

henkanother_hulk
	PUSHP	a0

henkget_pos
	movi	henkmax_x-henkmin_x,a0
	calla	RNDRNG0
	move	a0,a9
	movi	henkmax_y-henkmin_y,a0
	calla	RNDRNG0
	move	a0,a10

	addi	henkmin_x,a9
	addi	henkmin_y,a10

	cmpi	henkmid_x1,a9
	jrle	henkplace_hulk
	cmpi	henkmid_x2,a9
	jrge	henkplace_hulk
	cmpi	henkmid_y1,a10
	jrle	henkplace_hulk
	cmpi	henkmid_y2,a10
	jrge	henkplace_hulk
	jruc	henkget_pos

henkplace_hulk
	;assign a random starting position
	CREATE	HULK_PID,hulk
	PULLP	a0
	dsj	a0,henkanother_hulk

henkdone
	rets



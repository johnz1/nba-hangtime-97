 SUBR	quark_die
	PUSH	a1,a7

	movi	CLSDEAD,a14
	move	a14,*a8(OID),W
	move	*a8(OPLINK),a0,L
	movi	DEADQUARK_PID,a1
	movi	cexsbeen_shot,a7
	calla	XFERPROC

	PULL	a1,a7
	rets



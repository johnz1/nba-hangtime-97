	move	@bkbrd_proc_flg,a14
	jrnz	qjzmdie

	movk	1,a14
	move	a14,@bkbrd_proc_flg

	SLEEPK	7

	move	@ballrimhitcnt,a14
	jrnz	qjzmdone
	move	*a8(OXVEL),a0,L
	abs	a0			;Should we make a bbrd bounce sound?
	cmpi	01fffH,a0		;!!! Min XVEL to make sound
	jrlt	qjzmsoft

	SLEEPK	15

	move	@ballscorezhit,a0
	jrnz	qjzmdone			;br=ball hit score zone
	move	@ballrimhitcnt,a14
	cmpi	4,a14
	jrhs	qjzmdone

	movk	6,a0
	calla	rndrng0
	sll	5,a0
	addi	qjzmbckbd_hit_sp_tbl,a0
	move	*a0,a0,L
	calla	snd_play1
	SLEEP	70
	jruc	qjzmdone
qjzmsoft
	movi	250,a0
	calla	RNDPER
	jrls	qjzmdone
	SOUND1	soft_glas_sp
	SLEEP	70
qjzmdone
	clr	a14
	move	a14,@bkbrd_proc_flg
qjzmdie

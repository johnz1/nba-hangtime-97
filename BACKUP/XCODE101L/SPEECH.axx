*
* INPUT: reg a0 - player number
*
 SUBR	best_plyr_speech

	PUSH	a0
	movi	hlftm_ldr_sp,a14,L
	move	@gmqrtr,a1
	srl	2,a1
	jrz	xeszbsp_1				;br=halftime
	movi	gmevr_ldr_sp,a14,L

xeszbsp_1	movi	sound_ram,a1,L
	move	a14,*a1+,L
	move	*a14(16),a14,W
	move	a14,*a1+,W

	move	@spch_name_ram+32,a14,L		;plyr 1 is best
	subk	1,a0
	jrlt	xeszsyp
	move	@spch_name_ram+96,a14,L		;plyr 2
	subk	1,a0
	jrlt	xeszsyp
	move	@spch_name_ram+160,a14,L	;plyr 4
	subk	1,a0
	jrlt	xeszsyp
	move	@spch_name_ram+224,a14,L	;plyr 3
xeszsyp
	move	a14,*a1+,L

	movi	-1,a14
	move	a14,*a1,W

	movi	sound_ram,a0,L
	calla	script_play1
	PULL	a0
	rets




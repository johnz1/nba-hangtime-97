	movi	fwgqpass_spch_far_tbl,a2
fwgqpastos
	movk	7,a0
	calla	rndrng0
	sll	5,a0
	add	a2,a0
	move	*a0,a0,L
	jrz	fwgqfail
	calla	snd_play1
	jruc	fwgqfail
fwgqtoname
	move	@last_name_time,a0	;# ticks since name called
	cmpi	TSEC*3,a0
	jrls	fwgqfail

	move	a2,a1
	sll	5,a1
	addi	spch_pass_ram,a1
	move	*a1,a0,L
